	journal = {Environmental Research Letters},
	author = {Boser, Anna and Sousa, Daniel and Larsen, Ashley and MacDonald, Andrew},
	month = nov,
	year = {2021},
	note = {Publisher: IOP Publishing},
	pages = {124014},
