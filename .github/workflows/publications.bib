@article{khagram_thinking_2010,
	title = {Thinking about knowing: conceptual foundations for interdisciplinary environmental research},
	volume = {37},
	issn = {1469-4387, 0376-8929},
	shorttitle = {Thinking about knowing},
	url = {https://www.cambridge.org/core/journals/environmental-conservation/article/thinking-about-knowing-conceptual-foundations-for-interdisciplinary-environmental-research/D0A87C303D352CF3861D63D85D1CEF31},
	doi = {10.1017/S0376892910000809},
	abstract = {Working across knowledge-based research programmes, rather than institutional structures, should be central to interdisciplinary research. In this paper, a novel framework is proposed to facilitate interdisciplinary research, with the goals of promoting communication, understanding and collaborative work. Three core elements need to be addressed to improve interdisciplinary research: the types (forms and functions) of theories, the underlying philosophies of knowledge and the combination of research styles; these three elements combine to form the research programme. Case studies from sustainability science and environmental security illustrate the application of this research programme-based framework. This framework may be helpful in overcoming often oversimplified distinctions, such as qualitative/quantitative, deductive/inductive, normative/descriptive, subjective/objective and theory/practice. Applying this conceptual framework to interdisciplinary research should foster theoretical advances, more effective communication and better problem-solving in increasingly interdisciplinary environmental fields.},
	language = {en},
	number = {4},
	urldate = {2022-10-02},
	journal = {Environmental Conservation},
	author = {Khagram, Sanjeev and Nicholas, Kimberly A. and Bever, Dena Macmynowski and Warren, Justin and Richards, Elizabeth H. and Oleson, Kirsten and Kitzes, Justin and Katz, Rebecca and Hwang, Rebeca and Goldman, Rebecca and Funk, Jason and Brauman, Kate A.},
	month = dec,
	year = {2010},
	note = {Publisher: Cambridge University Press},
	keywords = {environment and security, environmental studies, epistemology, interdisciplinary research, philosophy of science, research methods, sustainability science, theory development},
	pages = {388--397},
	annote = {Annotated Notes
