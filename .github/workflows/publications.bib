	journal = {Environmental science \& technology},
	author = {Boaggio, Katie and LeDuc, Stephen D. and Rice, Byron and Duffney, Parker and Foley, Kristen M. and Holder, Amara and McDow, Stephen and Weaver, Christopher P.},
