	keywords = {reservoirs, water resiliency, water supplies},
	pages = {20150172},
	file = {Full Text PDF:/Users/emrok/Zotero/storage/8V4JVMJR/Martin - 2016 - At the nexus of fire, water and society.pdf:application/pdf},
